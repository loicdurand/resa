{% extends 'base.html.twig' %}

{% block body %}

	<h2 class="fr-mt-4w">Ajouter un véhicule</h2>

	<div class="fr-grid-row">

		{{ form_start(form, { 'attr' : { 'class': 'fr-grid-row fr-grid-row--gutters' } } ) }}


		{{ form_rest(form) }}

		<ul class="fr-mt-4w fr-btns-group fr-btns-group--inline-md">

			<li>
				<button type="reset" class="fr-btn fr-btn--secondary">
					Réinitialiser le formulaire
				</button>
			</li>

			<li>
				<button type="submit" class="fr-btn">
					Ajouter le véhicule
				</button>
			</li>

		</ul>

		{{ form_end(form) }}
	</div>

{% endblock %}
